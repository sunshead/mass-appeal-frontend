{
  "name": "react-starter-kit",
  "repository": "react-starter-kit",
  "version": "1.1.0",
  "engines": {
    "node": "6.10.1"
  },
  "description": "Intrepid starter kit for React with Redux applications",
  "main": "webpack.config.js",
  "scripts": {
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --config=config/webpack.development.babel.js",
    "test": "node scripts/test.js --env=jsdom",
    "build": "node_modules/.bin/webpack-dev-server --config=config/webpack.build.babel.js",
    "initial-setup": "rm -rf .git && git init && git add -A && git commit -m 'Initialize repository' && yarn install",
    "precommit": "lint-staged"
  },
  "author": "David Rodriguez Fuentes <davidrf.developer@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.27.2",
    "dotenv": "^4.0.0",
    "enzyme": "^2.7.1",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "fetch-mock": "^5.9.4",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.4.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "prettier": "^0.22.0",
    "react-addons-test-utils": "^15.4.2",
    "react-page-object": "^1.0.11",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "sass-loader": "4.1.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "1.0.1",
    "history": "^4.6.1",
    "humps": "^2.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "materialize-css": "^0.99.0",
    "normalizr": "^3.2.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "next",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write --single-quote --trailing-comma all",
      "git add"
    ]
  }
}
